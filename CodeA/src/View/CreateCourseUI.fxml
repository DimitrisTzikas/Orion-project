<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CreateCourseCON">
    <stylesheets>
        <URL value="@/CSS/createcourseui.css" />
    </stylesheets>
   <children>
      <Label layoutX="49.0" layoutY="62.0" text="Title:" />
      <Label layoutX="49.0" layoutY="97.0" text="Type:" />
      <Label layoutX="49.0" layoutY="130.0" text="Points:" />
      <Button fx:id="createCourseButton" layoutX="357.0" layoutY="226.0" mnemonicParsing="false" onAction="#createCourse" text="Create Course" />
      <Button fx:id="cancelButton" layoutX="62.0" layoutY="239.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <TextField fx:id="titleInput" layoutX="103.0" layoutY="57.0" onKeyTyped="#changeAction" />
      <ComboBox fx:id="comType" layoutX="103.0" layoutY="92.0" onAction="#changeAction" prefWidth="150.0" />
      <ComboBox fx:id="comPoints" layoutX="103.0" layoutY="125.0" onAction="#changeAction" prefWidth="150.0" />
      <Label layoutX="29.0" layoutY="167.0" text="Semester:" />
      <ComboBox fx:id="comSemester" layoutX="103.0" layoutY="162.0" onAction="#changeAction" prefWidth="150.0" />
      <Label fx:id="output" layoutX="395.0" layoutY="84.0" text="Outpu" />
   </children>
</AnchorPane>
