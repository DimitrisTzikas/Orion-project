<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddCourseCON">
    <stylesheets>
        <URL value="@/CSS/addcourseui.css" />
    </stylesheets>
   <children>
      <ComboBox fx:id="comCourses" layoutX="60.0" layoutY="45.0" prefWidth="150.0" />
      <Button fx:id="cancelButton" layoutX="60.0" layoutY="215.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Button fx:id="addCourseButton" layoutX="210.0" layoutY="215.0" mnemonicParsing="false" onAction="#addCourse" text="Add" />
   </children>
</AnchorPane>
